<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2934;"
            xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.sklep.pracownicyController">
    <children>
        <Label fx:id="label_sklep" layoutX="33.0" layoutY="42.0" prefHeight="17.0" prefWidth="186.0" text="STRONA GŁÓWNA"
               onMouseClicked="#goToMainPage">
            <font>
                <Font name="Arial Black" size="12.0"/>
            </font>
        </Label>

        <!-- Etykiety nad informacjami -->
        <Label text="Imię:" layoutX="111.0" layoutY="88.0" alignment="CENTER">
            <font>
                <Font name="Arial" size="14.0"/>
            </font>
            <style>
                -fx-font-weight: bold;
            </style>
        </Label>
        <Label text="Nazwisko:" layoutX="241.0" layoutY="88.0" alignment="CENTER">
            <font>
                <Font name="Arial" size="14.0"/>
            </font>
            <style>
                -fx-font-weight: bold;
            </style>
        </Label>
        <Label text="Kontakt:" layoutX="371.0" layoutY="88.0" alignment="CENTER">
            <font>
                <Font name="Arial" size="14.0"/>
            </font>
            <style>
                -fx-font-weight: bold;
            </style>
        </Label>

        <GridPane fx:id="gridPane" layoutX="111.0" layoutY="108.0" prefHeight="214.0" prefWidth="372.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
    </children>
</AnchorPane>
