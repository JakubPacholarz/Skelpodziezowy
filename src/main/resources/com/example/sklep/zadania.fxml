<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2934;"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.sklep.zadaniaController">
    <children>
        <Label fx:id="label_sklep" layoutX="33.0" layoutY="42.0" onMouseClicked="#goToMainPage" prefHeight="17.0" prefWidth="186.0"
               text="STRONA GŁÓWNA">
            <font>
                <Font name="Arial Black" size="12.0"/>
            </font>
        </Label>
        <AnchorPane prefHeight="400.0" prefWidth="600.0" layoutY="50"> <!-- Zmieniono layoutY na 50 -->
            <children>
                <Button layoutX="324.0" layoutY="10.0" mnemonicParsing="false" text="Start "/>
                <Button layoutX="403.0" layoutY="10.0" mnemonicParsing="false" text="Stop"/>
                <Label layoutX="528.0" layoutY="10.0" text="Czas"/>
                <ChoiceBox fx:id="pracownik" layoutX="122.0" layoutY="10.0" prefWidth="150.0" onMouseClicked="#addEmployeeToTable"/>
                <TableView fx:id="tableView" layoutY="40.0" prefHeight="360.0" prefWidth="600.0">
                    <columns>
                        <TableColumn fx:id="columnImie" prefWidth="75.0" text="Imię"/>
                        <TableColumn fx:id="columnNazwisko" prefWidth="75.0" text="Nazwisko"/>
                    </columns>
                </TableView>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
