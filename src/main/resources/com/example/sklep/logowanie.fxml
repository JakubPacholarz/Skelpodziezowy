<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox prefHeight="586.0" prefWidth="784.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sklep.LoginController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #2934;">
         <children>
            <ImageView fitHeight="181.0" fitWidth="159.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../../owl.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="586.0" prefWidth="784.0">
         <children>
            <Label layoutX="208.0" layoutY="145.0" prefHeight="35.0" prefWidth="172.0" text="ZALOGUJ SIĘ">
               <font>
                  <Font name="Arial Black" size="22.0" />
               </font>
               <style>
                  -fx-font-weight: bold;
               </style>
            </Label>
            <Label layoutX="265.0" layoutY="226.0" prefHeight="25.0" prefWidth="59.0" text="EMAIL:">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Label>
            <Label layoutX="265.0" layoutY="328.0" prefHeight="25.0" prefWidth="59.0" text="HASŁO:">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="tf_email" layoutX="220.0" layoutY="268.0" />
            <PasswordField fx:id="tf_password" layoutX="220.0" layoutY="365.0" />
            <Button fx:id="btn_login" layoutX="227.0" layoutY="439.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="39.0" prefWidth="135.0" style="-fx-background-color: #2934;" text="LOGOWANIE">
               <font>
                  <Font name="Arial Black" size="16.0" />
               </font>
            </Button>
            <Label fx:id="lbl_errorMessage" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="390.0" prefHeight="43.0" prefWidth="591.0" textFill="red" />

            <Label layoutX="61.0" layoutY="59.0" prefHeight="43.0" prefWidth="222.0" text="ZAJERESTRUJ SIĘ ">
               <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
            </Label>
            <Button fx:id="bt_rejestracja" layoutX="348.0" layoutY="59.0" mnemonicParsing="false" onAction="#registerButtonClicked" prefHeight="43.0" prefWidth="201.0" style="-fx-background-color: #2934;" text="REJESTRACJA">

            <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
            </Button>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />

         </children>
      </AnchorPane>
   </children>
</HBox>
